#include <iostream>
using namespace std;

long tersi(long k){
    long a, b;
    a = 0;
    while( k > 0 ){
        b = k % 10;
        a = a * 10 + b;
        k = k / 10;
    }
    return 0; 
}

int main(){
    long s;
    cout << " sayı girin: " ; 
    cin >> s;
    cout << endl;
    cout << " sayının tersi: " << tersi(s) << endl;
    return 0;
    
}
