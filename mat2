#include <iostream>
using namespace std; 

int main() {
    int a[3][3];
    int b[3][3];
    int c[3][3];

    for (int i=0; i<3; ++i)   
        for(int j=0; j<3; j++)
            {
                cout << "A matrisi değerleri" << i + 1 << j + 1 << " : ";
                cin >> a[i][j];
            }
            
    for (int i=0; i<3; ++i)   
        for(int j=0; j<3; j++)
            {
                cout << "B matrisi değerleri" << i + 1 << j + 1 << " : ";
                cin >> b[i][j];
            }
            
    for(int i = 0; i < 3; ++i)
        {
        for(int j = 0; j < 3; ++j){
            c[i][j] = a[i][j] + b[i][j];
            
            cout<<c[i][j]<<endl;
            }
            
            cout<< '\n';
        }
}
